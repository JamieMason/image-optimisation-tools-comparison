  "devDependencies": {
    "browserify": "12.0.1",
    "classnames": "2.2.1",
    "cz-conventional-changelog": "1.1.5",
    "esformatter": "0.8.1",
    "esformatter-braces": "1.2.1",
    "esformatter-dot-notation": "1.3.1",
    "esformatter-jsx": "^4.1.0",
    "esformatter-parseint": "1.0.3",
    "esformatter-quotes": "1.0.3",
    "esformatter-remove-trailing-commas": "1.0.1",
    "esformatter-semicolons": "1.1.2",
    "esformatter-use-strict": "2.0.2",
    "esformatter-var-each": "2.1.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.13.1",
    "fixpack": "2.2.0",
    "gulp": "3.9.0",
    "gulp-sass": "2.1.1",
    "image-optimisation-tools-comparison": "0.13.0",
    "react": "0.14.5",
