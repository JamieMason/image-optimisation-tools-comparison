{
  "name": "image-optimisation-tools-comparison",
  "description": "A benchmarking suite for popular image optimisation tools.",
  "version": "0.13.0",
  "author": "Jamie Mason <siunmaru@gmail.com>",
  "bugs": {
    "url": "https://github.com/JamieMason/image-optimisation-tools-comparison/issues"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-plugin-transform-async-to-generator": "6.3.13",
    "cz-conventional-changelog": "1.1.5",
    "deferred": "0.7.4",
    "glob": "6.0.3",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-imagemin": "1.0.0",
    "grunt-imageoptim": "1.4.1",
    "prettier": "1.5.2",
    "xo": "0.18.2"
  },
  "homepage": "https://github.com/JamieMason/image-optimisation-tools-comparison",
  "keywords": [
    "codekit",
    "image_optim",
    "imagemin",
    "imageoptim",
    "jpegmini",
    "jpegtran",
    "kraken.io",
    "pngquant",
    "smush.it"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:JamieMason/image-optimisation-tools-comparison.git"
  },
  "scripts": {
    "format": "prettier --write --single-quote 'src/**/*.js' '*.js'",
    "image_optim": "image_optim -r images/image_optim/",
    "imagemin": "grunt imagemin",
    "imageoptim": "grunt imageoptim",
    "lint": "xo --fix"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": true,
    "space": 2
  }
}
